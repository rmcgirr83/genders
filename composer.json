{
    "name": "rmcgirr83/genders",
    "type": "phpbb-extension",
    "description": "This Extension will allow your members to specify their Gender using fontawesome gender icons.",
    "version": "1.3.1",
    "time": "2024-07-17",
    "keywords": [
        "phpbb",
        "extension",
        "genders",
        "communication"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Genders",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/phpbb_3.1_genders",
            "filename": "version_check",
            "ssl": true
        }
    }
}